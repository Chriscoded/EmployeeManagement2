@if (ViewData["SuccessTitle"] == null)
{
    <h3 class="text-success">
        Your action was processed successfully
    </h3>
}
else
{
    <div class="alert alert-success text-align-content-center  mt-5"> 
        <h1 class="text-success">@ViewData["SuccessTitle"]</h1>
        <h6 class="text-success">@ViewData["SuccessTitle"]</h6>
    </div>
}
<hr/>
